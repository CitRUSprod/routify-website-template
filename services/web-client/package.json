        "@types/js-cookie": "2.2.6",
        "@types/nprogress": "0.2.0",
        "npm-run-all": "4.1.5",
        "snowpack": "3.0.13",
        "snowpack-plugin-hash": "0.14.2",
        "svelte-check": "1.2.6",
